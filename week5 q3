#include <iostream>
using namespace std;

void sum(int*a, int n ,int key)
{
  for(int i=0;i<n;i++)
        {
            for(int j=i+1;j<n;j++)
            {
                if(a[i]+a[j]==key)
                { cout<<a[i]<<","<<a[j]<<endl;
                }
            }
        }
  
}
void input(int *a, int n)
{
    for (int i = 0; i < n; i++)
    {
        cin >> a[i];
    }
}
int main()
{
    int t;

        int n;
        cin >> n;
        int a[n];
        input(a, n);
        cin>>t;
        sum(a,n,t)    ; 
    }

